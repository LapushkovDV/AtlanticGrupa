  MyWriteLN('    }                                                                                  ');
  MyWriteLN('    else                                                                               ');
  MyWriteLN('    {                                                                                  ');
  MyWriteLN('     $null                                                                             ');
  MyWriteLN('    }                                                                                  ');
  MyWriteLN('}                                                                                      ');
  MyWriteLN('                                                                                       ');
  MyWriteLN('                                                                                       ');
  MyWriteLN('[xml]$stockEntryElementList = $null                                                    ');
  MyWriteLN('[int]$npp = 1                                                                                 ');
  MyWriteLN(' write-host "Запрос GETPRODUCTTYPE. Отправка запроса " $NPP.ToString()  -ForegroundColor Green');
  MyWriteLN('Add-Content -Path $fileLog -Value $($(Get-Date).ToString("yyyy-MM-dd HH:mm:ss") + "Запрос GETPRODUCTTYPE. Отправка запроса " +$NPP.ToString()) -Encoding Oem');
  MyWriteLN('[xml]$stockEntryElementList = GetResponse -productType $npp.ToString()                        ');
  MyWriteLN('$item = New-Object PSObject                                                                   ');
  MyWriteLN('while ($stockEntryElementList -ne $null)                                                      ');
  MyWriteLN('{                                                                                                ');
  MyWriteLN('        foreach($product in $stockEntryElementList.Envelope.Body.getProductByTypeListResponse.productList.product)   ');
  MyWriteLN('        {                                                                                                            ');
  MyWriteLN('            $item = New-Object PSObject                                                                              ');
