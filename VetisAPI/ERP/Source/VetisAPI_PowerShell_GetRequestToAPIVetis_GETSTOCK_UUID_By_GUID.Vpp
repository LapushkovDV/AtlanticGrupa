procedure fillrecArray_GetStockUUIDbyGUID;
{
    var _npp: byte;
        _npp := 1;
    SetLimit(_recArray, 0);
    SetLimit(_recArray, 1);

  _recArray[_npp].def := coPS1_GetStockUUIDbyGUID_GUID         ; SetLimit(_recArray, GetLimit(_recArray) + 1); _npp++;
  _recArray[_npp].def := coPS1_GetStockUUIDbyGUID_UUID         ; SetLimit(_recArray, GetLimit(_recArray) + 1); _npp++;
  _recArray[_npp].def := coPS1_GetStockUUIDbyGUID_UNITGUID; SetLimit(_recArray, GetLimit(_recArray) + 1); _npp++;
  _recArray[_npp].def := co_PS1_REQUESTERROR   ;

}
Procedure Insert_GetStockUUIDbyGUID;
{
  var _i: byte;

  clearBuffer(tntStockGUIDUUID);
  for(_i := 1; _I <= getLimit(_recArray); _i++)
  {
    case upcase(_recArray[_i].def) of
      Upcase(coPS1_GetStockUUIDbyGUID_GUID )    : tStockGUIDUUID.guid  := Trim(_recArray[_i].value);
      Upcase(coPS1_GetStockUUIDbyGUID_UUID )    : tStockGUIDUUID.uuid  := Trim(_recArray[_i].value);
      Upcase(co_PS1_REQUESTERROR)               : tStockGUIDUUID.Error := Trim(_recArray[_i].value);
      Upcase(coPS1_GetStockUUIDbyGUID_UNITGUID) : tStockGUIDUUID.UNITGUID := Trim(_recArray[_i].value);
    end;
  }
     Insert Current tStockGUIDUUID;
}  // tInsertSaldoMC
