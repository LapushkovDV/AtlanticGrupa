  MyWriteLN('     $currentStatus = $XmlResponse.GetElementsByTagName(''status'').Item(0).FirstChild.data                                             ');
  MyWriteLN('     if ($currentStatus -eq "COMPLETED")                                                                                              ');
  MyWriteLN('      {                                                                                                                               ');
  MyWriteLN('       $isContinue = $false                                                                                                           ');
  MyWriteLN('       $result = $XmlResponse                                                                                                         ');
  MyWriteLN('       # write-host $response.content                                                                                                 ');
  MyWriteLN('      }                                                                                                                               ');
  MyWriteLN('     if ($currentStatus -eq "REJECTED")                                                                                               ');
  MyWriteLN('      {                                                                                                                               ');
  MyWriteLN('       $isContinue = $false                                                                                                           ');
  MyWriteLN('       $result = $null                                                                                                                ');
  MyWriteLN('       write-host "Запрос отклонен"                                                                                                   ');
  MyWriteLN('        $GlobError = $XmlResponse.Envelope.Body.receiveApplicationResultResponse.application.errors.error.''#text''                   ');
  MyWriteLN('      }                                                                                                                               ');
  MyWriteLN('                                                                                                                                      ');
  MyWriteLN('    }                                                                                                                                 ');
  MyWriteLN('    else                                                                                                                              ');
  MyWriteLN('    {                                                                                                                                 ');
  MyWriteLN('     $null                                                                                                                            ');
  MyWriteLN('    }                                                                                                                                 ');
  MyWriteLN('    #write-host "try number $CountTRy of $circuitCountTry | Curent state: $currentStatus"                                             ');
  MyWriteLN('    write-host "Запрос GetStock UUID by GUID. Прослушивание ответа API. Запрос $CountTRy из $circuitCountTry. Получен ответ: $currentStatus"  -ForegroundColor Green');
  MyWriteLN('    if ($circuitCountTry -lt $CountTRy )                                                 ');
  MyWriteLN('     {                                                                                   ');
  MyWriteLN('      $isContinue = $false                                                               ');
  MyWriteLN('     }                                                                                   ');
  MyWriteLN('  }                                                                                      ');
  MyWriteLN(' $result                                                                                 ');
  MyWriteLN('}                                                                                        ');
  MyWriteLN('[xml]$stockEntryElementList = $null                                                      ');
  MyWriteLN(' write-host "Запрос GetStock UUID by GUID. Отправка запроса"  -ForegroundColor Green                     ');
  MyWriteLN('$requestAplId = GetResponse                                                              ');
  MyWriteLN('if ($GlobError -eq "")                                                                   ');
  MyWriteLN('{                                                                                        ');
  MyWriteLN(' write-host "Запрос GetStock UUID by GUID. Прослушивание ответа API"  -ForegroundColor Green             ');
  MyWriteLN(' $stockEntryElementList = RecieveApplRqst($requestAplId)                                 ');
  MyWriteLN('                                                                                         ');
  MyWriteLN('}                                                                                        ');
  MyWriteLN('else                                                                                     ');
  MyWriteLN('{                                                                                        ');
  MyWriteLN(' $GlobError                                                                              ');
  MyWriteLN('}                                                                                        ');
  MyWriteLN('                                                                                         ');
  MyWriteLN('$FinalReport = $null                                                                     ');
  MyWriteLN('                                                                                         ');
  MyWriteLN('if ($stockEntryElementList -ne $null)                                                    ');
  MyWriteLN('{                                                                                 ');
  MyWriteLN('  $Response = $stockEntryElementList                                              ');
  MyWriteLN(' Write-host "Запрос stock by guid. Разбор XML"  -ForegroundColor Green            ');
  MyWriteLN('[System.Collections.ArrayList] $FinalReport = @();                                ');
  MyWriteLN('  foreach($stockEntry in $stockEntryElementList.Envelope.Body.receiveApplicationResultResponse.application.result.getStockEntryByGuidResponse.stockEntry) ');
  MyWriteLN('    {                                                                                                                                                     ');
  MyWriteLN('     $item = New-Object PSObject                                                                                                                           ');
  MyWriteLN('     $item | Add-Member -type NoteProperty -Name "'+coPS1_GetStockUUIDbyGUID_GUID+'" -value $stockEntry.Guid              ');
  MyWriteLN('     $item | Add-Member -type NoteProperty -Name "'+coPS1_GetStockUUIDbyGUID_UUID+'" -value $stockEntry.uuid             ');
  MyWriteLN('     $item | Add-Member -type NoteProperty -Name "'+co_PS1_REQUESTERROR+'" -value $GlobError.ToString()             ');
  MyWriteLN('     $item | Add-Member -type NoteProperty -Name "'+coPS1_GetStockUUIDbyGUID_UNITGUID +'" -value $stockEntry.batch.unit.guid');

  MyWriteLN('     $FinalReport +=,$item                                                                                      ');
  MyWriteLN('    }                                                                                                               ');
  MyWriteLN('[string]$sqlQuery = "" ');
  MyWriteLN(' [int]$npp = 1                                                                       ');
  MyWriteLN(' write-host "Запрос GETSTOCK UUID by GUID. Запись в буферную таблицу" -ForegroundColor Green         ');
  MyWriteLN(' foreach($item in $FinalReport)                                                      ');
  MyWriteLN('  {                                                                                  ');
  MyWriteLN('   if([math]::Truncate($npp/23) -eq $npp/23) ');
  MyWriteLN('    {                                       ');
  MyWriteLN('     write-host "Запись в буферную таблицу полученных результатов" -ForegroundColor Cyan       ');
  MyWriteLN('     $res = Invoke-DatabaseQuery -connectionString $conStrinGal -query $sqlQuery -isSQLServer ');
  MyWriteLN('     $sqlQuery = "" ');
  MyWriteLN('    }                                       ');

  MyWriteLN('   write-host "Запрос GetStock UUID by GUID. Запись запроса " $npp.tostring()  -ForegroundColor Cyan       ');
  MyWriteLN('   foreach( $property in $item.psobject.properties.name )                            ');
  MyWriteLN('    {                                                                                ');
  MyWriteLN('      if ($item.$property -eq $null)                                                 ');
  MyWriteLN('        {                                                                            ');
  MyWriteLN('         $itemproperty = ""                                                          ');
  MyWriteLN('        }                                                                            ');
  MyWriteLN('        else                                                                         ');
  MyWriteLN('         {                                                                           ');
  MyWriteLN('          $itemproperty = $item.$property                                            ');
  MyWriteLN('         }                                                                           ');
  MyWriteLN('      $sqlQuery = $sqlQuery + ''Insert t$ATL_RQSTAPIVALUES (f$RQST_GUID, f$RSQT_PROVIDER, f$NROW, f$COLUMNNAME, f$VALUE)    ');
  MyWriteLN('                   values (''''''+$RQST_GUID+'''''',''''''+$circuitserviceID+'''''',''+$npp.tostring()+'',''''''+$property.Replace("''","''''")+'''''',''''''+$itemproperty.Replace("''","''''")+'''''') ');
  MyWriteLN('                    ''                                                                            ');
  MyWriteLN('    }                                                                                          ');
  MyWriteLN('    $npp = $npp + 1                                                                            ');
  MyWriteLN('  }                                                                                            ');
  MyWriteLN('   write-host "Запись в буферную таблицу полученных результатов" -ForegroundColor Cyan       ');
  MyWriteLN('      $res = Invoke-DatabaseQuery -connectionString $conStrinGal -query $sqlQuery -isSQLServer ');
  MyWriteLN('      $sqlQuery = "" ');

  MyWriteLN('}                                                                                                                                      ');

}
Function GetRequestToAPIVetis_GetStockUUIDbyGUID(_GUID: string; var _UUID, _UNITGUID: string) : boolean;
{
  _rqstError := '';
  delete all tStockGUIDUUID;
   #BodyProcedureGetRqst(GetStockUUIDbyGUID,_GUID)

  IF (getfirst tStockGUIDUUID where tStockGUIDUUID.GUID = _GUID) = tsOK
   {
     _UUID     := tStockGUIDUUID.UUID;
     _UNITGUID := tStockGUIDUUID.UNITGUID;
     Result := true;
     MyLog('По GUID '+_GUID+' найден stock uuid ' +_UUID);
   }
   else
   {
     Result := false;
    _UUID := tStockGUIDUUID.Error;
    _UNITGUID := '';
     MyLog('ОШИБКА: ' + tStockGUIDUUID.Error);
   }
}
